{
    "name": "Homeqtt",
    "accessory": "homebridge-homeqtt-alarm",
    "debug": true,
    "mqttConfig": {
        "url": "mqtt://192.168.1.90:1883",
        "username": "USERNAME",
        "password": "PASSWORD"
    },
    "alarmTopics": {
        "messageTopic": "tele/rfbridge/RESULT",
        "setTargetStateTopic": "alarm/target",
        "getCurrentStateTopic": "alarm/current"
    },
    "alarmSettings": {
        "targetStates": {
            "stayArm": true,
            "awayArm": true,
            "nightArm": true
        }
    },
    "sensor": [
        {
            "sensorType": "pirSensor",
            "location": "Front Room PIR",
            "MQTTCode": "F5678E",
            "enabled": true,
            "allowStay": false,
            "allowAway": true,
            "allowNight": true
        },
        {
            "sensorType": "contactSensor",
            "location": "Back Door",
            "MQTTCode": "09F5A4",
            "enabled": true,
            "allowStay": true,
            "allowAway": true,
            "allowNight": true
        }
    ],
    "keyfob": true,
    "keyfobs": [
        {
            "enabled": true,
            "name": "Dad Keyfob",
            "buttons": [
                {
                    "enabled": false,
                    "alarmState": 0,
                    "MQTTCode": "D88C84",
                    "rfkeyTopic": "cmnd/rfbridge/rfkey1"
                },
                {
                    "enabled": true,
                    "alarmState": 1,
                    "MQTTCode": "D88C81",
                    "rfkeyTopic": "cmnd/rfbridge/rfkey2"
                },
                {
                    "enabled": true,
                    "alarmState": 3,
                    "MQTTCode": "D88C82",
                    "rfkeyTopic": "cmnd/rfbridge/rfkey3"
                },
                {
                    "enabled": true,
                    "alarmState": 4,
                    "MQTTCode": "D88C88",
                    "rfkeyTopic": "cmnd/rfbridge/rfkey4"
                }
            ]
        },
        {
            "enabled": false,
            "name": "Mum Keyfob",
            "buttons": [
                {
                    "enabled": false,
                    "alarmState": 0,
                    "MQTTCode": "F99C84",
                    "rfkeyTopic": "cmnd/rfbridge/rfkey5"
                },
                {
                    "enabled": true,
                    "alarmState": 1,
                    "MQTTCode": "F99C81",
                    "rfkeyTopic": "cmnd/rfbridge/rfkey6"
                },
                {
                    "enabled": true,
                    "alarmState": 3,
                    "MQTTCode": "F99C82",
                    "rfkeyTopic": "cmnd/rfbridge/rfkey7"
                },
                {
                    "enabled": true,
                    "alarmState": 4,
                    "MQTTCode": "F99C88",
                    "rfkeyTopic": "cmnd/rfbridge/rfkey8"
                }
            ]
        }
    ],
    "siren": {
        "enabled": true
    }
}